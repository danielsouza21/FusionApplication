{
  "FusionCache": {
    "UseBackplaneDistributed": false,
    "DefaultDurationMinutes": 2,
    "DefaultDurationSeconds": 0,
    "DefaultDurationMilliseconds": 0,
    "DefaultJitterMaxDurationSeconds": 20,
    "DefaultJitterMaxDurationMilliseconds": 0,
    "DefaultFailSafeEnabled": true,
    "DefaultFailSafeMaxDurationHours": 1,
    "DefaultFailSafeMaxDurationMinutes": 0,
    "DefaultFactorySoftTimeoutMilliseconds": 150,
    "DefaultFactoryHardTimeoutSeconds": 2,
    "DefaultFactoryHardTimeoutMilliseconds": 0,
    "DefaultEagerRefreshThreshold": 0.15,
    "DefaultPriority": "Normal",
    "SetSystemTextJsonSerializer": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FusionCacheApplication.Infrastructure.Database.Repositories.EfUserRepository": "Information",
      "FusionCacheApplication.Application.Services.UserService": "Information",
      "FusionCacheApplication.Infrastructure.Database.DatabaseMigrationService": "Information",
      "FusionCacheApplication.Configuration.FusionCacheConfiguration": "Information"
    }
  },
  "AllowedHosts": "*"
}
